{
  "name": "lumenday-oaktest",
  "version": "0.1.0",
  "description": "Lumenday test application for OakOS.",
  "main": "src/index.js",
  "engines": {
    "node": "8.11.3"
  },
  "dependencies": {
    "@oaklabs/platform": "~1.3.0",
    "lodash": "~4.17.10",
    "standard": "~12.0.1"
  },
  "devDependencies": {
    "oak": "~4.3.4"
  },
  "scripts": {
    "start": "node_modules/.bin/oak src/index.js",
    "rebuild": "/bin/bash -c 'node_modules/.bin/oak-rebuild'",
    "dev": "export $(cat .env | xargs) && npm start",
    "test-standard": "node_modules/.bin/standard",
    "clean-install": "rm -rf node_modules package-lock.json && npm i && npm run rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lumenday/lumenday-oaktest.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/lumenday/lumenday-oaktest/issues"
  },
  "homepage": "https://github.com/lumenday/lumenday-oaktest#readme",
  "private": true
}
